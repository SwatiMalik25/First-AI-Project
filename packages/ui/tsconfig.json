{
  "extends": "@repo/typescript-config/react-library.json",
  "exclude": ["dist",  "node_modules"],
  "include": [ "components", "lib", "styles"],
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "paths": {
      "@/*": ["./*"],
          }
  }
}
